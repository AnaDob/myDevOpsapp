pipeline {
    agent any
    tools {
        maven 'Maven 3.3.9'
    }

        stage ('Build') {
            steps {
                sh 'mvn -Dmaven.test.failure.ignore=true install' 
            }
        }
        
        stage ('Run') {
            steps {
                script {
                    sh 'bin/lavagna.sh'
                }
            }
        }
    }


